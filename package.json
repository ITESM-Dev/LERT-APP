    "expo-app-loading": "~2.0.0",
    "expo-font": "~10.1.0",
    "expo-status-bar": "~1.3.0",
    "jest": "^26.6.3",
    "jest-expo": "^45.0.1",
    "native-base": "^3.3.10",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
